<h1>New gif</h1>

<%= form_for @gif do |f| %>
  <% if @gif.errors.any? %>
    <div class="error">
    <%= "Url can't be blank" %>
    </div>
    <div class="error">
    <%= "Title can't be blank" %>
    </div>
  <% end %>
<%= f.label :url %>
<%= f.text_field :url %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.submit "Create gif" %>

<% end %>